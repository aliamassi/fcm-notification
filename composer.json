{
    "name": "aliamassi/fcm-notification",
    "description": "Laravel package to send Firebase Cloud Messaging notifications to Android and iOS.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AliAmassi\\FcmNotification\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "google/auth": "^1.20",
        "guzzlehttp/guzzle": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AliAmassi\\FcmNotification\\FirebaseNotificationServiceProvider"
            ],
            "aliases": {
                "FirebaseNotification": "AliAmassi\\FcmNotification\\Facades\\FirebaseNotification"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
